{
  "name": "hookstack",
  "version": "0.0.1",
  "description": "A growing collection of practical, production-ready React hooks built for modern React and Node.js, with a focus on simplicity, correctness, and real-world reuse.",
  "keywords": [
    "react",
    "react-hooks",
    "hooks",
    "custom-hooks",
    "react-utils",
    "react-library",
    "frontend",
    "typescript",
    "web",
    "ui",
    "state",
    "utilities"
  ],
  "license": "MIT",
  "author": "Gautam Suthar <iamgautamsuthar@outlook.com> (https://github.com/callmegautam)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callmegautam/hookstack.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint 'src/**' --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\""
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.7",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "prettier": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@10.11.1",
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
